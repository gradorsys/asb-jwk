{
  "name": "asb-jwk",
  "version": "0.0.0-semantically-released",
  "description": "library for json web keys (JWKs)",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "watch:test": "mocha src/index.test.js -w",
    "test": "istanbul cover -x **/*.test.js _mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gradorsys/asb-jwk.git"
  },
  "keywords": [
    "jwk"
  ],
  "author": "Gunnar Radzom <gunnar.radzom@adorsys.de> (http://adorsys.de/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gradorsys/asb-jwk/issues"
  },
  "homepage": "https://github.com/gradorsys/asb-jwk#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
